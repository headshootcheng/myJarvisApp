{
  "expo": {
    "name": "my-jarvis",
    "slug": "my-jarvis",
    "version": "1.2",
    "orientation": "portrait",
    "icon": "./assets/images/config/icon.png",
    "owner": "codingbadas",
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "assets/images/config/icon.png",
          "color": "#ffffff",
          "sounds": [
            "assets/sounds/bellSound.wav"
          ],
          "mode": "production"
        }
      ]
    ],
    "splash": {
      "image": "./assets/images/config/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.headshootcheng.my-jarvis",
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/config/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/images/config/favicon.png"
    }
  }
}
